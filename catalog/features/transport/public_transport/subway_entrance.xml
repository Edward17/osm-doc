<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="subway_entrance">
        <title lang="ru">Вход в метро</title>
        <tags>
            <tag>
                <key>railway</key>
                <val value="subway_entrance"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Доступ на инвалидной коляске</title>
                <key>wheelchair</key>
                <val value="yes">
                    <title lang="ru">есть</title>
                </val>
                <val value="limited">
                    <title lang="ru">ограниченный</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="designated">
                    <title lang="ru">специально предназначено</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Дата открытия</title>
                <key>start_date</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Проход с велосипедом</title>
                <key>bicycle</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
            </tag>
        </more-tags>
        <icon>subway_entrance.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
