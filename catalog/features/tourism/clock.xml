<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="clock">
        <title lang="ru">Часы</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="clock"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Видимость</title>
                <key>visibility</key>
                <val value="area">
                    <title lang="ru">с большого расстояния</title>
                </val>
                <val value="street">
                    <title lang="ru">в пределах улицы</title>
                </val>
                <val value="house">
                    <title lang="ru">в пределах нескольких метров</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Метод отображения</title>
                <key>display</key>
                <val value="sundial">
                    <title lang="ru">солнечные часы</title>
                </val>
                <val value="unorthodox">
                    <title lang="ru">нестандартный</title>
                </val>
                <val value="analog">
                    <title lang="ru">аналоговый (стрелки)</title>
                </val>
                <val value="digital">
                    <title lang="ru">цифровой</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Число циферблатов</title>
                <key>faces</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Показывает влажность</title>
                <key>hygrometer</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Метод установки</title>
                <key>support</key>
                <val value="pole">
                    <title lang="ru">крепление на столб</title>
                </val>
                <val value="wall_mounted">
                    <title lang="ru">крепление на стену</title>
                </val>
                <val value="ground">
                    <title lang="ru">на земле</title>
                </val>
                <val value="billboard">
                    <title lang="ru">информационный щит</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Показывает дату</title>
                <key>date</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Показывает давление</title>
                <key>barometer</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Показывает температуру</title>
                <key>thermometer</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
        </more-tags>
        <icon>clock.png</icon>
        <applyed-to>node way area</applyed-to>
    </feature>
</doc-part>
