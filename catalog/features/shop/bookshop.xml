<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="bookshop">
        <title lang="ru">Книжный</title>
        <tags>
            <tag>
                <key>shop</key>
                <val value="books"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Продажа б/у</title>
                <key>second_hand</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="only">
                    <title lang="ru">только б/у</title>
                </val>
            </tag>
        </more-tags>
        <icon>bookshop.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
