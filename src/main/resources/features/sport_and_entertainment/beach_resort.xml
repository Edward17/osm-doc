<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="beach_resort">
        <title lang="ru">Пляжный комплекс</title>
        <description>Оборудованный пляж с услугами</description>
        <tags>
            <tag>
                <key>leisure</key>
                <val value="beach_resort"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Наличие платы</title>
                <key>fee</key>
                <val value="yes">
                    <title lang="ru">платный</title>
                </val>
                <val value="no">
                    <title lang="ru">бесплатный</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Нудизм</title>
                <key>nudism</key>
                <val value="yes">
                    <title lang="ru">Да</title>
                </val>
                <val value="no">
                    <title lang="ru">Нет</title>
                </val>
                <val value="permissive">
                    <title lang="ru">Не возбраняется</title>
                </val>
            </tag>
