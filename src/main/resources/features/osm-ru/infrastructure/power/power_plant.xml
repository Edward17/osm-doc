<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="power_plant">
        <title>Электростанция</title>
        <tags>
            <tag>
                <key>plant:output:electricity</key>
                <val match="any" value="*"/>
            </tag>
            <tag>
                <key>power</key>
                <val value="plant"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <icon>power_plant.png</icon>
        <applyed-to area="true" node="true"/>
    </feature>
</doc-part>
