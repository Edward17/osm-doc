<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="parking">
        <title lang="ru">Парковка</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="parking"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Наличие платы</title>
                <key>fee</key>
                <val value="yes">
                    <title lang="ru">платный</title>
                </val>
                <val value="no">
                    <title lang="ru">бесплатный</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Тип парковки</title>
                <key>parking</key>
                <val value="garage_boxes">
                    <title lang="ru">гаражи</title>
                </val>
                <val value="surface">
                    <title lang="ru">открытая</title>
                </val>
                <val value="carports">
                    <title lang="ru">навесы</title>
                </val>
                <val value="sheds">
                    <title lang="ru">ракушки и пеналы</title>
                </val>
                <val value="underground">
                    <title lang="ru">подземная</title>
                </val>
                <val value="multi-storey">
                    <title lang="ru">многоэтажная</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Ограничение по времени стоянки</title>
                <key>maystay</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Перехватывающая парковка</title>
                <key>park_ride</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="bus">
                    <title lang="ru">пересадка на автобус</title>
                </val>
                <val value="ferry">
                    <title lang="ru">пересадка на паром</title>
                </val>
                <val value="metro">
                    <title lang="ru">пересадка на метро</title>
                </val>
                <val value="train">
                    <title lang="ru">пересадка на поезд</title>
                </val>
                <val value="tram">
                    <title lang="ru">пересадка на трамвай</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Количество машиномест</title>
                <key>capacity</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Крытый</title>
                <key>covered</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
        </more-tags>
        <icon>parking.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
