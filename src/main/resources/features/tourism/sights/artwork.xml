<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="artwork">
        <title lang="ru">Арт-объект</title>
        <tags>
            <tag>
                <key>tourism</key>
                <val value="artwork"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Имя художника</title>
                <key>artist_name</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Дата установки</title>
                <key>start_date</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Тип произведения</title>
                <key>artwork_type</key>
                <val value="sculpture">
                    <title lang="ru">скульптура</title>
                </val>
                <val value="statue">
                    <title lang="ru">статуя</title>
                </val>
                <val value="mural">
                    <title lang="ru">фреска</title>
                </val>
                <val value="architecture">
                    <title lang="ru">архитектурный объект</title>
                </val>
                <val value="mosaic">
                    <title lang="ru">мозаика</title>
                </val>
                <val value="painting">
                    <title lang="ru">живопись</title>
                </val>
            </tag>
        </more-tags>
        <icon>artwork.png</icon>
        <applyed-to>node way area</applyed-to>
    </feature>
</doc-part>
