<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="clock">
        <title>Часы</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="clock"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Видимость</title>
                <key>visibility</key>
                <val value="area">
                    <title>с большого расстояния</title>
                </val>
                <val value="street">
                    <title>в пределах улицы</title>
                </val>
                <val value="house">
                    <title>в пределах нескольких метров</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Метод отображения</title>
                <key>display</key>
                <val value="sundial">
                    <title>солнечные часы</title>
                </val>
                <val value="unorthodox">
                    <title>нестандартный</title>
                </val>
                <val value="analog">
                    <title>аналоговый (стрелки)</title>
                </val>
                <val value="digital">
                    <title>цифровой</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title>Число циферблатов</title>
                <key>faces</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Показывает влажность</title>
                <key>hygrometer</key>
                <val value="yes">
                    <title>да</title>
                </val>
                <val value="no">
                    <title>нет</title>
                </val>
                <val value="true">
                    <title>да</title>
                </val>
                <val value="false">
                    <title>нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Метод установки</title>
                <key>support</key>
                <val value="pole">
                    <title>крепление на столб</title>
                </val>
                <val value="wall_mounted">
                    <title>крепление на стену</title>
                </val>
                <val value="ground">
                    <title>на земле</title>
                </val>
                <val value="billboard">
                    <title>информационный щит</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Показывает дату</title>
                <key>date</key>
                <val value="yes">
                    <title>да</title>
                </val>
                <val value="no">
                    <title>нет</title>
                </val>
                <val value="true">
                    <title>да</title>
                </val>
                <val value="false">
                    <title>нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Показывает давление</title>
                <key>barometer</key>
                <val value="yes">
                    <title>да</title>
                </val>
                <val value="no">
                    <title>нет</title>
                </val>
                <val value="true">
                    <title>да</title>
                </val>
                <val value="false">
                    <title>нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title>Показывает температуру</title>
                <key>thermometer</key>
                <val value="yes">
                    <title>да</title>
                </val>
                <val value="no">
                    <title>нет</title>
                </val>
                <val value="true">
                    <title>да</title>
                </val>
                <val value="false">
                    <title>нет</title>
                </val>
            </tag>
        </more-tags>
        <icon>clock.png</icon>
        <applyed-to>node way area</applyed-to>
    </feature>
</doc-part>
