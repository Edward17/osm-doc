<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="power_plant">
        <title lang="ru">Электростанция</title>
        <tags>
            <tag>
                <key xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:match="any">plant:output:electricity</key>
                <val value="*"/>
            </tag>
            <tag>
                <key>power</key>
                <val value="plant"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка холодной воды</title>
                <key>plant:output:cold_water</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Источник энергии/топливо</title>
                <key>generator:source</key>
                <val value="waste">
                    <title lang="ru">мусор, отходы</title>
                </val>
                <val value="wind">
                    <title lang="ru">ветряная энергия</title>
                </val>
                <val value="biomass">
                    <title lang="ru">биомасса</title>
                </val>
                <val value="gasoline">
                    <title lang="ru">безнзин</title>
                </val>
                <val value="biogas">
                    <title lang="ru">биогаз</title>
                </val>
                <val value="biofuel">
                    <title lang="ru">биотопливо</title>
                </val>
                <val value="osmotic">
                    <title lang="ru">осмотическая энегрия</title>
                </val>
                <val value="solar">
                    <title lang="ru">солнечная энергия</title>
                </val>
                <val value="hydro">
                    <title lang="ru">гидроэнегрия</title>
                </val>
                <val value="oil">
                    <title lang="ru">мазут</title>
                </val>
                <val value="gas">
                    <title lang="ru">природный газ</title>
                </val>
                <val value="diesel">
                    <title lang="ru">дизльное топливо</title>
                </val>
                <val value="nuclear">
                    <title lang="ru">ядерная энегрия</title>
                </val>
                <val value="coal">
                    <title lang="ru">уголь</title>
                </val>
                <val value="geothermal">
                    <title lang="ru">геотермальная энегрия</title>
                </val>
                <val value="wave">
                    <title lang="ru">приливная энегрия</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка пара</title>
                <key>plant:output:steam</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка вакуума</title>
                <key>plant:output:vacuum</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка сжатого воздуха</title>
                <key>plant:output:compressed_air</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка электроэнергии</title>
                <key>plant:output:electricity</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Способ получения энергии</title>
                <key>generator:method</key>
                <val value="photovoltaic">
                    <title lang="ru">фотовольтаика</title>
                </val>
                <val value="stream">
                    <title lang="ru">использование приливных течений</title>
                </val>
                <val value="thermal">
                    <title lang="ru">нагрев солнцем</title>
                </val>
                <val value="fusion">
                    <title lang="ru">ядерный синтез</title>
                </val>
                <val value="water-pumped-storage">
                    <title lang="ru">использование водохранилища с закачкой воды (ГАЭС)</title>
                </val>
                <val value="combustion">
                    <title lang="ru">сжигание топлива</title>
                </val>
                <val value="run-of-the-river">
                    <title lang="ru">использование течения реки</title>
                </val>
                <val value="fission">
                    <title lang="ru">ядерный распад</title>
                </val>
                <val value="gasification">
                    <title lang="ru">газификация</title>
                </val>
                <val value="barrage">
                    <title lang="ru">использование изменений уровня водной поверхности</title>
                </val>
                <val value="water-storage">
                    <title lang="ru">использование водохранилища</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка горячей воды</title>
                <key>plant:output:hot_water</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка холодного воздуха</title>
                <key>plant:output:cold_air</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Выработка горячего воздуха</title>
                <key>plant:output:hot_air</key>
            </tag>
        </more-tags>
        <icon>power_plant.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
