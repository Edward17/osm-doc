<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<doc-part xmlns="http://map.osm.me/osm-doc-part">
    <feature name="post_office">
        <title lang="ru">Почта</title>
        <tags>
            <tag>
                <key>amenity</key>
                <val value="post_office"/>
            </tag>
        </tags>
        <trait>poi</trait>
        <more-tags>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Доступ в интернет</title>
                <key>internet_access</key>
                <val value="yes">
                    <title lang="ru">Есть</title>
                </val>
                <val value="wlan">
                    <title lang="ru">WiFi</title>
                </val>
                <val value="terminal">
                    <title lang="ru">Терминал</title>
                </val>
                <val value="no">
                    <title lang="ru">Нет</title>
                </val>
                <val value="wired">
                    <title lang="ru">Проводное подключение</title>
                </val>
                <val value="public">
                    <title lang="ru">Есть</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Оператор доступа в интернет</title>
                <key>internet_access:operator</key>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="enum">
                <title lang="ru">Плата за доступ в интернет</title>
                <key>internet_access:fee</key>
                <val value="yes">
                    <title lang="ru">да</title>
                </val>
                <val value="no">
                    <title lang="ru">нет</title>
                </val>
                <val value="true">
                    <title lang="ru">да</title>
                </val>
                <val value="false">
                    <title lang="ru">нет</title>
                </val>
            </tag>
            <tag xmlns:ns0="http://map.osm.me/osm-doc-part" ns0:tag-value-type="number">
                <title lang="ru">Номер отделения</title>
                <key>ref</key>
            </tag>
        </more-tags>
        <icon>post_office.png</icon>
        <applyed-to>node area</applyed-to>
    </feature>
</doc-part>
